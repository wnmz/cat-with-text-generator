{
  "name": "cats-generator",
  "version": "1.0.2",
  "description": "Simple package that will help you to generate cat images with text",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "tsc && node lib/examples/saveImage.js",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "canvas": "^2.9.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "keywords": [
    "cat-generator",
    "cats",
    "cat",
    "images",
    "text",
    "generate",
    "generator",
    "cat",
    "generator",
    "canvas",
    "pussy",
    "kittens"
  ],
  "author": "wnmz",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/wnmz/cats-with-text-generator.git"
